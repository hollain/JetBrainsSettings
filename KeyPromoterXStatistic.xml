<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'⌘F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Path">
        <Item>
          <IdeaActionID>CopyPaths</IdeaActionID>
          <ShortCut>'⇧⌘C'</ShortCut>
          <Description>Copy Path</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'⌘X' or '⇧⌦'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Favorites</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Parameter Info">
        <Item>
          <IdeaActionID>ParameterInfo</IdeaActionID>
          <ShortCut>'⌘P'</ShortCut>
          <Description>Parameter Info</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Save All">
        <Item>
          <IdeaActionID>SaveAll</IdeaActionID>
          <ShortCut>'⌘S'</ShortCut>
          <Description>Save All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'⌘7'</ShortCut>
          <Description>Structure</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>